@model KevinPass.Models.Pc_1Model
@{
    ViewBag.Title = "Boleta";
}
<form method="post" asp-action="Registra">
    <div class="form-group"></div>
        <label for="nomb">Nombre</label>
        <input type="text" class="form-control" id="Nomb" name="Nomb" required />
    </div>
    <div class="form-group"></div>
        <label for="apel">Apellido</label>
        <input type="text" class="form-control" id="apel" name="Apel" required />
    </div>
    <div class="form-group"></div>
        <label for="dni">Dni</label>
        <input type="text" class="form-control" id="dni" name="Dni" required />
    </div>
    <button type="submit" class="btn btn-primary mt-2">Confirmar</button>
</form>
<h1>@ViewData["Message"]</h1>